$+file+           ->  expand('%')
$+path+           ->  expand('%:p')
$+desc+           ->  '...'
$+author+         ->  'i am i'
$+email+          ->  'myemail@email.com'
$+homepage+       ->  'my homepage'
$+version+        ->  '1.0'
$+bash_version+   ->  system('echo -n $BASH_VERSION')
$+last-modified+  ->  strftime('%c')
